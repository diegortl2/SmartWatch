  <Requirements:Requirement xmi:id="_9J-HgPCgEemoOf5U7hYdsA" base_Class="_9GzewPCgEemoOf5U7hYdsA" id="R-01" text="The smart watch should tell the user the time and allow the user to excecute, install or uninstall widgets which can make use of it's sensors."/>
  <Requirements:Requirement xmi:id="_RJ7ckPCmEemoOf5U7hYdsA" base_Class="_RJ0u4PCmEemoOf5U7hYdsA" id="R-02" text="The watch shall display the current time when the screen is not asleep and when the screen is not displaying another widget at the moment. "/>
  <Requirements:DeriveReqt xmi:id="_RKIQ4PCmEemoOf5U7hYdsA" base_DirectedRelationship="_RKCKQPCmEemoOf5U7hYdsA" base_Abstraction="_RKCKQPCmEemoOf5U7hYdsA"/>
  <Requirements:Requirement xmi:id="_Lq5LkPCnEemoOf5U7hYdsA" base_Class="_Lqyd4PCnEemoOf5U7hYdsA" id="R-03" text="Screen should be asleep to save battery and it can be turned on by tapping the touch screen, pressing the side button or detecting a rotation to check the watch (face up)."/>
  <Requirements:DeriveReqt xmi:id="_LrBucPCnEemoOf5U7hYdsA" base_DirectedRelationship="_Lq8O4PCnEemoOf5U7hYdsA" base_Abstraction="_Lq8O4PCnEemoOf5U7hYdsA"/>
  <Requirements:Requirement xmi:id="_Mbt5oPCpEemoOf5U7hYdsA" base_Class="_MbpoMPCpEemoOf5U7hYdsA" id="R-04" text="The watch should let the user install, uninstall or excecute widgets which provide data extracted from the sensors."/>
  <Requirements:DeriveReqt xmi:id="_MbzZMPCpEemoOf5U7hYdsA" base_DirectedRelationship="_MbvHwPCpEemoOf5U7hYdsA" base_Abstraction="_MbvHwPCpEemoOf5U7hYdsA"/>
  <Requirements:Requirement xmi:id="_44acoPCpEemoOf5U7hYdsA" base_Class="_44YAYPCpEemoOf5U7hYdsA" id="R-05" text="The GPS sensor shall allow widgets to retrieve the location coordinates and time. "/>
  <Requirements:DeriveReqt xmi:id="_44fVIPCpEemoOf5U7hYdsA" base_DirectedRelationship="_44c44PCpEemoOf5U7hYdsA" base_Abstraction="_44c44PCpEemoOf5U7hYdsA"/>
  <Requirements:Requirement xmi:id="_KDJEMPFjEemXarYm_2DuEw" base_Class="_KC5zoPFjEemXarYm_2DuEw" id="R-06" text="The Acelerometer sensor shall allow widgets to detect sudden movements and the movement direction on a vector form. "/>
  <Requirements:DeriveReqt xmi:id="_KDUDUPFjEemXarYm_2DuEw" base_DirectedRelationship="_KDN8sPFjEemXarYm_2DuEw" base_Abstraction="_KDN8sPFjEemXarYm_2DuEw"/>
  <Requirements:Requirement xmi:id="_vNpzAPFjEemXarYm_2DuEw" base_Class="_vNmIoPFjEemXarYm_2DuEw" id="R-07" text="The gyroscope sensor shall allow widgets to detect device rotation and provide it's direction on a vector form. "/>
  <Requirements:DeriveReqt xmi:id="_vNurgPFjEemXarYm_2DuEw" base_DirectedRelationship="_vNsPQPFjEemXarYm_2DuEw" base_Abstraction="_vNsPQPFjEemXarYm_2DuEw"/>
  <Requirements:Requirement xmi:id="_ILCdoPFkEemXarYm_2DuEw" base_Class="_IK_aUPFkEemXarYm_2DuEw" id="R-08" text="The photodiode sensor shall allow widgets to detect light conditions. "/>
  <Requirements:DeriveReqt xmi:id="_ILGvEPFkEemXarYm_2DuEw" base_DirectedRelationship="_ILDrwPFkEemXarYm_2DuEw" base_Abstraction="_ILDrwPFkEemXarYm_2DuEw"/>
